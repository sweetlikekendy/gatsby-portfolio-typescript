{
  "name": "gatsby-portfolio-typescript",
  "version": "1.0.0",
  "description": "Gatsby example site demonstrating how to use TypeScript with Gatsby",
  "author": "Kendy Nguyen <kendyisnguyen@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-serve": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "develop": "gatsby clean && gatsby develop",
    "develop-host": "gatsby clean && gatsby develop -H 0.0.0.0",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "lint-fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  },
  "sideEffects": false,
  "dependencies": {
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@sanity/block-content-to-react": "^2.0.7",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "flexsearch": "github:nextapps-de/flexsearch#0.7.0",
    "gatsby": "^2.19.36",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-advanced-sitemap": "^1.6.0",
    "gatsby-plugin-anchor-links": "^1.2.1",
    "gatsby-plugin-emotion": "^5.4.0",
    "gatsby-plugin-google-gtag": "^2.8.0",
    "gatsby-plugin-local-search": "^2.0.0",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-typography": "^3.0.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-source-sanity": "^6.0.5",
    "gatsby-transformer-json": "^2.11.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.1",
    "react-icons": "^4.2.0",
    "react-typography": "^0.16.19",
    "react-use-flexsearch": "^0.1.1",
    "twin.macro": "^2.2.3",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-typescript": "^7.13.0",
    "@emotion/babel-plugin": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^6.1.0",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^10.2.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-preset-gatsby": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.22.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "postcss": "^8.2.7",
    "prettier": "2.1.1",
    "react-syntax-highlighter": "^15.4.3",
    "tailwindcss": "^2.0.3",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sweetlikekendy/gatsby-portfolio-typescript/"
  },
  "bugs": {
    "url": "https://github.com/sweetlikekendy/gatsby-portfolio-typescript/issues"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion",
      "config": "./src/tailwind.config.js"
    }
  }
}
